<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<!-- Module name affects the location of generated Javascript files and RPC routing paths. -->
<module rename-to="example">
    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="com.google.gwt.i18n.I18N"/>

    <!-- GWT material -->
    <inherits name="gwt.material.design.GwtMaterialWithJQueryDebug" />

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
    <!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
    <!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/>     -->

    <entry-point class="com.jingyu.example.client.ExampleEntryPoint" />

    <!-- Specify the paths for translatable code -->
    <source path="client" />
    <source path="shared" />
</module>